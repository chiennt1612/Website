@using Microsoft.AspNetCore.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@model IEnumerable<EntityFramework.Web.Entities.ParamSetting>

@{ 
    var requestCulture = Context.Features.Get<IRequestCultureFeature>().RequestCulture.Culture.Name;
    var Email = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "EMAIL").FirstOrDefault().ParamValue;
    var HOTLINE = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "HOTLINE").FirstOrDefault().ParamValue;
    var PHONE = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "PHONE").FirstOrDefault().ParamValue;
    var WEBSITE = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "WEBSITE").FirstOrDefault().ParamValue;
    var HOTLINEENGINEER = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "HOTLINEENGINEER").FirstOrDefault().ParamValue;
    var HOTLINESALE = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "HOTLINESALE").FirstOrDefault().ParamValue;
    var FACEBOOKLINK = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "FACEBOOKLINK").FirstOrDefault().ParamValue;
    var ZALOLINK = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "ZALOLINK").FirstOrDefault().ParamValue;
    var YOUTUBELINK = Model.Where(u => (u.Language == requestCulture || u.Language == "all") && u.ParamKey == "YOUTUBELINK").FirstOrDefault().ParamValue;
}

